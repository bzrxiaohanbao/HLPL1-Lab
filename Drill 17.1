//Write a function to print the value of a to OS
ostream& print_ar(ostream& os, int* a, int n)
{
 for (int i = 0; i < n; ++i) {
  os << a[i] << " ";
 } 
 os << endl;
 return os;
}
ostream& print_vector(ostream& os, vector<int>& v)
{
 for (auto e : v) {
  os << e << " ";
 }  
 os << endl;
 return os;
}
