#include "std_lib_facilities.h" 
//1.Define vector < int > and initialize it
vector<int> vt = { 1, 2, 4, 8, 16, 32, 64, 128, 256, 512 };
//2. Define function fun
void fun(vector<int> vt)
{ 
	vector<int> lv(vt.size()); 
	lv = vt;
	//3.print. 
	for (auto& e : lv) {
		cout << e << " ";
	}	
	cout << endl; 
	vector<int> lv2 = vt;
	//print
	for (auto& e : lv2) {
		cout << e << " ";
	}
	cout << endl;
}
int main()
try {
	//4.call the fun 
	fun(vt);
	int x = 1;
	vector<int> vv(10);
	for (int i = 0; i < vv.size(); ++i){
		x *= (i + 1);
		vv[i] = x;
	}
	//4.use the fun  
	fun(vv);
	return 0;
}
catch (exception& e)
{
	cerr << "Exception! " << e.what() << endl;
	return 1;
}
catch (...)
{
	cerr << "Some exceptions! " << endl;
	return 2;
}
